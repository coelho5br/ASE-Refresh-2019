{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json",
    "contentVersion": "1.0.0.0",    
    "variables": {        
        "certificateName3": "DefaultCertificateFor_ASE-REFRESH-3_InternalLoadBalancingASE",
        "certificateThumbprint3": "9be260bfa86e74ecc3396829a94996e97e4b1222",
        "pfxBlobString3": "",
        "password3": "No_P@ssw0rd!",
        "aseName3": "ASE-REFRESH-3",
        "_location": "[resourceGroup().location]"
    },    
    "resources": [
          {
            "apiVersion": "2015-08-01",
            "type": "Microsoft.Web/certificates",
            "name": "[variables('certificateName3')]",
            "location": "[variables('_location')]",
            "dependsOn": [
              "[concat('Microsoft.Web/hostingEnvironments/',variables('aseName3'))]"
